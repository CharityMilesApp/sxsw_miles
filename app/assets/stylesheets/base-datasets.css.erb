table {
	margin: 0 auto;
}

table tbody td {
	padding: 0 0 40px 0;
	text-align: center;
	vertical-align: middle;
	font-weight: 100;
}

table tbody td.name {
	text-align: left;
	padding-left: 20px;
}

table tbody td.miles {
	text-align: left;
	padding-left: 20px;
}

table tbody td.raised {
	font-size: 46px;
}

table tbody td.pie {
	padding-left: 55px;
}

table tbody tr:last-child td { padding-bottom: 0; }